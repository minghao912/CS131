(class foo
 (method void f ((int x)) (print x))
)
(class bar inherits foo
 (method void f ((int x) (int y)) (print x " " y))
)

(class main
 (field bar b null)
 (method void main ()
   (begin
     (set b (new bar))
     (call b f 10)  	# calls version of f defined in foo
     (call b f 10 20)   # calls version of f defined in bar
   )
 )
)
