(class a
    (field int x 5)
    (method int test ()
        (return x)
    )
)

(class b inherits a
    (field string str "string")
    (method string test ()
        (return str)
    )
)

(class main
    (field a a_ null)
    (field b b_ null)

    (method void main ()
        (begin
            (set a_ (new b))
            (set b_ a_)

            (print (call a_ test))
        )
    )
)