(class person
    (field int age 5)
)

(class student inherits person
    (field string major "CS")
)

(class main
    (field student pf null)
    (method void foo ((person p1) (person p2)) 
        (if (== p1 p2)   
            (print "same object")
            (print "not same object")
        )
    )


    (method void main ()
        (begin
            (set pf (new student))
            (call me foo pf pf)
        )
    )
)
