(class person
    (field int age 5)
)

(class student inherits person
    (field string major "CS")
)

(class professor inherits person
    (field string class "CS131")
)

(class dog
    (field string name "Fido")
)

(class main
    (method void foo ((professor ref1) (student ref2)) 
        (if (== ref1 ref2)                               
            (print "same object")
            (print "not same object")
        )
    )


    (method void main ()
        (let ((student stu null))
            (set stu (new student))
            (call me foo (new person) stu)
        )
    )
)
