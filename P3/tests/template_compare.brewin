(tclass MyTemplate (person_type)
    (field person_type aPerson)
    (method void act ((person_type p))
        (call p act)
    )
    (method person_type getPerson ()
        (let ((person_type tmp__))
            (set tmp__ aPerson)
            (return tmp__)
        )
    )
)

(class Person
    (method void act ()
        (print "talk talk talk")
    )
)

(class main
    (field MyTemplate@Person pp)

    (method void main ()
        (let ((Person p_) (MyTemplate@Person tmp_ null))
            (set p_ (new Person))
            (set tmp_ (new MyTemplate@Person))
            # (set pp (new MyTemplate@Person))
            (if (== pp null)
                (print "equal")
                (print "nequal")
            )
        )
    )
)